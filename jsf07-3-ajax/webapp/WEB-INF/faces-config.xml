<?xml version="1.0" encoding="UTF-8"?>
<faces-config
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
    <application>
        <message-bundle>resources.mymessages</message-bundle>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>frutas/search</from-outcome>
            <to-view-id>/frutas/search.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>frutas/index</from-outcome>
            <to-view-id>/frutas/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
